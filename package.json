{
	"name": "tamachi-gatsby",
	"version": "0.1.0",
	"description": "Gatsby site for documentation or blogging",
	"author": "Alvaro Garcia",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "npm run develop",
		"prebuild": "npm run test",
		"build": "gatsby build",
		"develop": "gatsby develop --port 3000",
		"docker": "docker-compose up -d",
		"docker:stop": "docker-compose down",
		"serve": "gatsby serve --port 3000 -H 0.0.0.0",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"{src,content-demo}/**/*.{js,ts,tsx}\" \"*.js\"",
		"test:prettier": "prettier --check \"{src,types,content-demo}/**/*.{js,ts,tsx}\" \"*.js\"",
		"prettier": "prettier --write \"{src,types,content-demo}/**/*.{js,ts,tsx}\" \"*.js\""
	},
	"dependencies": {
		"@mdx-js/mdx": "^1.6.19",
		"@mdx-js/react": "^1.6.19",
		"@types/lodash.throttle": "^4.1.6",
		"@types/mdx-js__react": "^1.5.3",
		"@types/react": "^16.9.53",
		"@types/react-dom": "^16.9.8",
		"@types/react-helmet": "^6.1.0",
		"@types/remove-markdown": "^0.1.1",
		"@types/uuid": "^8.3.0",
		"copy-text-to-clipboard": "^2.2.0",
		"fuse.js": "^6.4.2",
		"gatsby": "^2.24.85",
		"gatsby-alias-imports": "^1.0.4",
		"gatsby-plugin-layout": "^1.3.13",
		"gatsby-plugin-manifest": "^2.4.35",
		"gatsby-plugin-mdx": "^1.2.51",
		"gatsby-plugin-offline": "^3.2.37",
		"gatsby-plugin-react-helmet": "^3.3.14",
		"gatsby-plugin-sharp": "^2.6.43",
		"gatsby-plugin-stylus": "^2.3.13",
		"gatsby-plugin-typescript": "^2.4.24",
		"gatsby-plugin-zopfli": "^2.0.0",
		"gatsby-remark-images": "^3.3.39",
		"gatsby-remark-prismjs": "^3.5.16",
		"gatsby-remark-unwrap-images": "^1.0.2",
		"gatsby-source-filesystem": "^2.3.35",
		"gatsby-transformer-remark": "^2.8.46",
		"lodash.throttle": "^4.1.1",
		"prismjs": "^1.22.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-helmet": "^6.1.0",
		"react-scrollbars-custom": "^4.0.25",
		"remove-markdown": "^0.3.0",
		"stylus": "^0.54.8",
		"typescript": "^4.0.3",
		"uuid": "^8.3.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.5.0",
		"@typescript-eslint/parser": "^4.5.0",
		"eslint": "^7.12.0",
		"eslint-config-prettier": "^6.14.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"prettier": "^2.1.2"
	}
}
