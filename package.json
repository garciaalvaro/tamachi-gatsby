{
	"name": "tamachi-gatsby",
	"version": "0.2.0",
	"description": "Gatsby site for documentation or blogging",
	"author": "Alvaro Garcia",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "npm run develop",
		"prebuild": "npm run test",
		"build": "gatsby build",
		"develop": "gatsby develop --port 3000",
		"docker": "docker-compose up -d",
		"docker:stop": "docker-compose down",
		"serve": "gatsby serve --port 3000 -H 0.0.0.0",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"{src,content-demo}/**/*.{js,ts,tsx}\" \"*.js\"",
		"test:prettier": "prettier --check \"{src,types,content-demo}/**/*.{js,ts,tsx}\" \"*.js\"",
		"prettier": "prettier --write \"{src,types,content-demo}/**/*.{js,ts,tsx}\" \"*.js\""
	},
	"dependencies": {
		"@mdx-js/mdx": "^1.6.21",
		"@mdx-js/react": "^1.6.21",
		"@types/lodash.throttle": "^4.1.6",
		"@types/mdx-js__react": "^1.5.3",
		"@types/react": "^16.14.0",
		"@types/react-dom": "^16.9.9",
		"@types/react-helmet": "^6.1.0",
		"@types/remove-markdown": "^0.1.1",
		"@types/uuid": "^8.3.0",
		"copy-text-to-clipboard": "^2.2.0",
		"fuse.js": "^6.4.3",
		"gatsby": "^2.27.0",
		"gatsby-alias-imports": "^1.0.6",
		"gatsby-plugin-layout": "^1.5.0",
		"gatsby-plugin-manifest": "^2.7.0",
		"gatsby-plugin-mdx": "^1.5.0",
		"gatsby-plugin-offline": "^3.5.0",
		"gatsby-plugin-react-helmet": "^3.5.0",
		"gatsby-plugin-sharp": "^2.9.0",
		"gatsby-plugin-stylus": "^2.5.0",
		"gatsby-plugin-typescript": "^2.7.0",
		"gatsby-plugin-zopfli": "^2.0.0",
		"gatsby-remark-images": "^3.6.0",
		"gatsby-remark-prismjs": "^3.8.0",
		"gatsby-remark-unwrap-images": "^1.0.2",
		"gatsby-source-filesystem": "^2.6.0",
		"gatsby-transformer-remark": "^2.11.0",
		"lodash.throttle": "^4.1.1",
		"prismjs": "^1.22.0",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-helmet": "^6.1.0",
		"react-scrollbars-custom": "^4.0.25",
		"remove-markdown": "^0.3.0",
		"stylus": "^0.54.8",
		"typescript": "^4.1.2",
		"uuid": "^8.3.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.8.1",
		"@typescript-eslint/parser": "^4.8.1",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"prettier": "^2.2.0"
	}
}
