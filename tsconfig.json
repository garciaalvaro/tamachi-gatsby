{
	"compilerOptions": {
		"target": "ESNext",
		"noEmit": true,
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"jsx": "react",
		"baseUrl": ".",
		"skipLibCheck": true,
		"paths": {
			"@/*": ["src/*"]
		},
		"types": ["cypress"]
	},
	"include": ["src", "types", "tests"]
}
